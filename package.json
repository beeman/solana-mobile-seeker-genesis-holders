{
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "@solana/kit": "^6.0.1",
    "@solana/kit-plugin-rpc": "^0.2.0",
    "drizzle-kit": "^0.31.9",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "latest"
  },
  "license": "MIT",
  "name": "solana-mobile-seeker-genesis-holders",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beeman/solana-mobile-seeker-genesis-holders.git"
  },
  "scripts": {
    "check-types": "tsc -b --noEmit",
    "db:migrate": "bunx drizzle-kit migrate",
    "dev": "bun run --hot src/api.ts",
    "docker:build": "docker build -t ghcr.io/beeman/solana-mobile-seeker-genesis-holders .",
    "docker:push": "docker push ghcr.io/beeman/solana-mobile-seeker-genesis-holders",
    "docker:run": "docker run --rm --name seeker-genesis-holders --init --stop-signal=SIGINT -it --env-file .env -p 3000:3000 ghcr.io/beeman/solana-mobile-seeker-genesis-holders",
    "index": "bun run src/index.ts",
    "index:all": "bun run src/index.ts --all",
    "index:epoch": "bun run src/index.ts --epoch",
    "lint": "biome check --error-on-warnings",
    "lint:fix": "biome check --error-on-warnings --write",
    "serve": "bun run src/api.ts",
    "start": "bun run db:migrate && bun run serve",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "type": "module",
  "version": "0.0.0"
}
